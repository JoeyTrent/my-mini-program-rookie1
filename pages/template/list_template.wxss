.temContainer{
  display: flex;
  flex-direction: column;
  border-top: 1rpx solid #eee;
  border-bottom: 1rpx solid #eee;
  margin: 10rpx 0;
}

.avatar_date image{
  width: 60rpx;
  height: 60rpx;
  vertical-align: middle;
  margin-left: 10rpx;
}

.avatar_date text{
  font-size: 32rpx;
}

.company {
  margin-left: 16rpx;
  font-size: 36rpx;
  font-weight: 700;
  margin: 16rpx;
}

.contentImg {
  width: 100%;
  height: 490rpx;
}

.content {
  padding: 20rpx;
  font-size: 32rpx;
  text-indent:32rpx;
}

.collection_love {
  margin: 10rpx 30rpx;
 }

.collection_love image{
  width: 32rpx;
  height: 32rpx;
  vertical-align: middle;
  margin-right: 16rpx;
}
.collection_love text{
  font-size: 28rpx;
  margin-right: 16rpx;
}